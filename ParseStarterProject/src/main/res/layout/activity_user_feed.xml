<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/lLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".UserList">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:weightSum="10">

        <TextView
            android:id="@+id/tvStatus"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_gravity="center"
            android:layout_weight="3"
            android:background="#58f205"
            android:gravity="center"
            android:text="User Not Loged IN"
            android:textColor="#000" />

        <TextView
            android:id="@+id/tvDate"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_gravity="center"
            android:layout_margin="5dp"
            android:layout_weight="7"
            android:gravity="center"
            android:text="Date"
            android:background="#ed540c"
            android:textColor="#000" />

    </LinearLayout>



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="3dp"
        android:orientation="horizontal"
        android:weightSum="10">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="3"
            android:gravity="center"
            android:text="Time"
            android:textSize="25dp"
            android:textStyle="bold|italic" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_weight="3.5"
            android:gravity="center"
            android:text="Production"
            android:textSize="17dp"
            android:textStyle="bold|italic" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_weight="3.5"
            android:gravity="center"
            android:text="Target"
            android:textSize="17dp"
            android:textStyle="bold|italic" />
    </LinearLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="10">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_weight="3"
                        android:gravity="center"
                        android:text="8:30 - 9:30 "
                        android:textSize="22dp"
                        android:textStyle="italic|bold" />

                    <TextView
                        android:id="@+id/tvPro1"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_marginRight="5dp"
                        android:layout_weight="3.5"
                        android:gravity="center"
                        android:inputType="number"
                        android:textSize="23dp"
                        android:textStyle="italic|bold" />

                    <TextView
                        android:id="@+id/tvTar1"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_weight="3.5"
                        android:background="#f9f8b6"
                        android:gravity="center"
                        android:inputType="number"

                        android:textColor="#000"
                        android:textSize="23dp"
                        android:textStyle="italic|bold" />


                </LinearLayout>


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="10">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_weight="3"
                        android:gravity="center"
                        android:text="9:30 - 10:30 "
                        android:textSize="22dp"
                        android:textStyle="italic|bold" />

                    <TextView
                        android:id="@+id/tvPro2"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_marginRight="5dp"
                        android:layout_weight="3.5"
                        android:gravity="center"
                        android:inputType="number"
                        android:textSize="23dp"
                        android:textStyle="italic|bold" />

                    <TextView
                        android:id="@+id/tvTar2"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_weight="3.5"
                        android:background="#f9f8b6"
                        android:gravity="center"
                        android:inputType="number"

                        android:textColor="#000"
                        android:textSize="23dp"
                        android:textStyle="italic|bold" />


                </LinearLayout>


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="10">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_weight="3"
                        android:gravity="center"
                        android:text="10:30 - 11:30 "
                        android:textSize="22dp"
                        android:textStyle="italic|bold" />

                    <TextView
                        android:id="@+id/tvPro3"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_marginRight="5dp"
                        android:layout_weight="3.5"
                        android:gravity="center"
                        android:inputType="number"
                        android:textSize="23dp"
                        android:textStyle="italic|bold" />

                    <TextView
                        android:id="@+id/tvTar3"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_weight="3.5"
                        android:background="#f9f8b6"
                        android:gravity="center"
                        android:inputType="number"
                        android:textColor="#000"
                        android:textSize="23dp"
                        android:textStyle="italic|bold" />


                </LinearLayout>


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="10">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_weight="3"
                        android:gravity="center"
                        android:text="11:30 - 12:30 "
                        android:textSize="22dp"
                        android:textStyle="italic|bold" />

                    <TextView
                        android:id="@+id/tvPro4"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_marginRight="5dp"
                        android:layout_weight="3.5"
                        android:gravity="center"
                        android:inputType="number"
                        android:textSize="23dp"
                        android:textStyle="italic|bold" />

                    <TextView
                        android:id="@+id/tvTar4"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_weight="3.5"
                        android:background="#f9f8b6"
                        android:gravity="center"
                        android:inputType="number"
                        android:textColor="#000"
                        android:textSize="23dp"
                        android:textStyle="italic|bold" />


                </LinearLayout>


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="10">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_weight="3"
                        android:gravity="center"
                        android:text="1:00 - 2:00 "
                        android:textSize="22dp"
                        android:textStyle="italic|bold" />

                    <TextView
                        android:id="@+id/tvPro5"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_marginRight="5dp"
                        android:layout_weight="3.5"
                        android:gravity="center"
                        android:inputType="number"
                        android:textSize="23dp"
                        android:textStyle="italic|bold" />

                    <TextView
                        android:id="@+id/tvTar5"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_weight="3.5"
                        android:background="#f9f8b6"
                        android:gravity="center"
                        android:inputType="number"
                        android:textColor="#000"
                        android:textSize="23dp"
                        android:textStyle="italic|bold" />


                </LinearLayout>


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="10">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_weight="3"
                        android:gravity="center"
                        android:text="2:00 - 3:00 "
                        android:textSize="22dp"
                        android:textStyle="italic|bold" />

                    <TextView
                        android:id="@+id/tvPro6"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_marginRight="5dp"
                        android:layout_weight="3.5"
                        android:gravity="center"
                        android:inputType="number"
                        android:textSize="23dp"
                        android:textStyle="italic|bold" />

                    <TextView
                        android:id="@+id/tvTar6"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_weight="3.5"
                        android:background="#f9f8b6"
                        android:gravity="center"
                        android:inputType="number"
                        android:textColor="#000"
                        android:textSize="23dp"
                        android:textStyle="italic|bold" />


                </LinearLayout>


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="10">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_weight="3"
                        android:gravity="center"
                        android:text="3:00 - 4:00 "
                        android:textSize="22dp"
                        android:textStyle="italic|bold" />

                    <TextView
                        android:id="@+id/tvPro7"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_marginRight="5dp"
                        android:layout_weight="3.5"
                        android:gravity="center"
                        android:inputType="number"
                        android:textSize="23dp"
                        android:textStyle="italic|bold" />

                    <TextView
                        android:id="@+id/tvTar7"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_weight="3.5"
                        android:background="#f9f8b6"
                        android:gravity="center"
                        android:inputType="number"
                        android:textColor="#000"
                        android:textSize="23dp"
                        android:textStyle="italic|bold" />


                </LinearLayout>


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="10">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_weight="3"
                        android:gravity="center"
                        android:text="4:00 - 5:00 "
                        android:textSize="22dp"
                        android:textStyle="italic|bold" />

                    <TextView
                        android:id="@+id/tvPro8"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_marginRight="5dp"
                        android:layout_weight="3.5"
                        android:gravity="center"
                        android:inputType="number"
                        android:textSize="23dp"
                        android:textStyle="italic|bold" />

                    <TextView
                        android:id="@+id/tvTar8"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_weight="3.5"
                        android:background="#f9f8b6"
                        android:gravity="center"
                        android:inputType="number"
                        android:textColor="#000"
                        android:textSize="23dp"
                        android:textStyle="italic|bold" />


                </LinearLayout>


            </LinearLayout>


        </LinearLayout>
    </ScrollView>

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/bRefresh"
        android:text="Refresh"/>



</LinearLayout>
